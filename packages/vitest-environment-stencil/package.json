{
  "name": "vitest-environment-stencil",
  "version": "1.1.12",
  "description": "Custom Vitest environment for Stencil component testing",
  "type": "module",
  "main": "./index.mjs",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs"
    }
  },
  "files": [
    "index.mjs",
    "index.d.ts"
  ],
  "keywords": [
    "vitest",
    "environment",
    "stencil",
    "testing"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/stenciljs/vitest",
    "directory": "packages/vitest-environment-stencil"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@stencil/vitest": "workspace:*"
  }
}
